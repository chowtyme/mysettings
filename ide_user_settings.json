"editor.tokenColorCustomizations": {
  "comments": "#51525a",      // Dark muted gray, out of the way
  "keywords": "#4e81c2",      // Muted, classic PowerShell blue
  "functions": "#b8b9bb",     // Darker off-white/gray
  "variables": "#679ec7",     // Steely blue, not too bright
  "strings": "#888a91",       // Muted, dark grayish (almost blends in, no orange pop)
  "numbers": "#6a9371",       // Muted desaturated green-gray
  "types": "#6a98a9",         // Muted blue-gray
  "classes": "#bba36a",       // Muted tan/gold, not bright (or use #929296 for even flatter)
  "parameters": "#b8b9bb",    // Gray, matches functions
  "properties": "#b8b9bb",    // Gray, blends with code
  "operators": "#b8b9bb",     // Gray, minimal distinction
  "enumMember": "#b8b9bb",    // Gray, keep flat
  "typeParameters": "#b8b9bb" // Gray, flat
},

